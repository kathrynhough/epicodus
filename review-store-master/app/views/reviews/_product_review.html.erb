  <%= form_for @review, :html => { :multipart => true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :content %>
  <%= f.text_field :content %>
  <%= f.label :rating %>
  <%= f.text_field :rating %>
  <%= f.hidden_field :user_id, :value => current_user.id  %>
  <%= f.submit "Save review", class: "button" %>
<% end %> 

